{
    "workbench.colorTheme": "Solarized Light",
    "emmet.triggerExpansionOnTab": true,
    "telemetry.enableTelemetry": false,
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "explorer.confirmDragAndDrop": false,
    "workbench.colorCustomizations": {
        "[Solarized Light]": {
            "editor.selectionBackground": "#fdd76d",
            "editor.findMatchBackground": "#ffcc41"
        }
    },
    "editor.dragAndDrop": false,
    "editor.multiCursorModifier": "alt",
    "files.autoSave": "onFocusChange",
    "editor.renderControlCharacters": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "window.restoreWindows": "all",
    "workbench.commandPalette.preserveInput": true,
    "workbench.quickOpen.preserveInput": true,
    "search.exclude": {
        ".next": true,
        ".server": true,
        "**/node_modules/.cache": true,
        "legacy/node_modules": true,
        "package-lock.json": true,
        "yarn.lock": true
    },
    "editor.acceptSuggestionOnCommitCharacter": false,
    "prettier.semi": false,
    "prettier.singleQuote": true,
    "prettier.tabWidth": 4,
    "prettier.trailingComma": "es5",
    "editor.suggestSelection": "first",
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.maxTokenizationLineLength": 25000,
    "window.zoomLevel": 0,
    "git.confirmSync": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "vsicons.dontShowNewVersionMessage": true,
    "terminal.integrated.env.osx": {
        "SOLARIZED_THEME": "light"
    },
    "terminal.integrated.shell.osx": "/bin/zsh",
    "diffEditor.ignoreTrimWhitespace": false,
    "terminal.integrated.scrollback": 100000,
    "editor.renderWhitespace": "selection",
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "eslint.codeAction.disableRuleComment": {
        "enable": true,
        "location": "separateLine"
    },
    "eslint.run": "onSave",
    "prettier.printWidth": 160,
    "editor.autoClosingQuotes": "never",
    "todo-tree.highlights.customHighlight": {
        "TODO": {},
        "FIXME": {}
    },
    "todo-tree.highlights.defaultHighlight": {
        "foreground": "green",
        "type": "none"
    },
    "files.associations": {
        "[Dd]ockerfile*": "dockerfile",
        "docker-compose*": "yaml",
        "*.hs": "haskell"
    },
    "dart.flutterSdkPath": "/Users/fabian/bin/flutter",
    "clipboard-manager.snippet.enabled": false,
    "yaml.schemas": {
        "file:///Users/fabian/.vscode/extensions/atlassian.atlascode-2.4.9/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
    },
    "atlascode.jira.jqlList": [
        {
            "id": "b1877057-8726-4e33-9198-b926032b73a5",
            "enabled": true,
            "name": "My jira.willhaben.at Issues",
            "query": "assignee = currentUser() AND resolution = Unresolved ORDER BY lastViewed DESC",
            "siteId": "jira.willhaben.at",
            "monitor": true
        }
    ],
    "atlascode.bitbucket.statusbar.showUser": false,
    "atlascode.bitbucket.statusbar.showLogin": true,
    "atlascode.bitbucket.statusbar.showProduct": false,
    "atlascode.jira.statusbar.showProduct": false,
    "atlascode.jira.statusbar.showUser": false,
    "todo-tree.tree.showScanModeButton": false,
    "timeline.showView": true,
    "diffEditor.renderSideBySide": true,
    "editor.gotoLocation.multipleTypeDefinitions": "goto",
    "todohighlight.include": ["**/*.js", "**/*.jsx", "**/*.ts", "**/*.tsx", "**/*.html", "**/*.php", "**/*.css", "**/*.scss", "**/*.less"],
    "todohighlight.keywords": ["TODO", "FIXME", "XXX"],
    "clipboard-manager.onlyWindowFocused": false,
    "editor.columnSelection": false,
    "typescript.enablePromptUseWorkspaceTsdk": true,
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.formatOnSave": true
}
